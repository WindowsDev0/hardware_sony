    <dimen name="battery_care_preview_height">405dp</dimen>

    <!-- H.S (Heat Suppression) Power Control QS Tile -->
    <string name="charger_qs_tile_title">충전 일시 중지</string>

    <!-- Refresh Rate QS Tile -->
    <string name="refresh_rate_tile_title">재생률</string>
